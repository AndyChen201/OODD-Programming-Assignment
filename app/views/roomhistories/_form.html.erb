<%= form_for(@roomhistory) do |f| %>
    <%= f.hidden_field :number, :value => params[:number] %>
    <%= f.hidden_field :time_from, :value => params[:time_from] %>
    <%= f.hidden_field :time_to, :value => params[:time_to] %>
    <%= f.label :userid %><br>
    <%= f.text_field :userid, class: 'form-control' %>

    <br><br>
    <%= f.submit yield(:button_text), class: "btn btn-primary" %>
<% end %>